{
  "manifest_version": 2,
  "name": "Faster",
  "description": "Control HTML video elements playback rate",
  "version": "1.0",

  "icons": {
    "16": "icons/faster-16.png",
    "24": "icons/faster-24.png",
    "32": "icons/faster-32.png",
    "48": "icons/faster-48.png",
    "128": "icons/faster-128.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/faster.js"]
    }
  ]
}
